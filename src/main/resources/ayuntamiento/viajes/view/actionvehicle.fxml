      <TextField fx:id="modelTF" layoutX="145.0" layoutY="113.0" promptText="Modelo" />
      <ComboBox fx:id="typeCB" layoutX="425.0" layoutY="113.0" prefWidth="150.0" visibleRowCount="5" />
