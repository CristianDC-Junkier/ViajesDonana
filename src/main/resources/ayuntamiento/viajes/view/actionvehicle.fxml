    <stylesheets>
        <URL value="@/ayuntamiento/viajes/styles/addvehicle.css" />
    </stylesheets>
   <children>
      <Label layoutX="43.0" layoutY="51.0" text="Matrícula:" />
      <Label layoutX="340.0" layoutY="51.0" text="Marca:" />
      <Label layoutX="43.0" layoutY="117.0" text="Modelo:" />
